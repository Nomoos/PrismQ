# Example .env file for CSV Import Source

# Database path
CSV_IMPORT_DATABASE_PATH=/path/to/PrismQ_WD/Sources/Internal/CSVImport/csv_import.db

# Default CSV file (optional)
CSV_IMPORT_DEFAULT_PATH=

# CSV parsing options
CSV_IMPORT_DELIMITER=,
CSV_IMPORT_ENCODING=utf-8

# Batch import settings
CSV_IMPORT_BATCH_SIZE=100

# Default values for missing fields
CSV_IMPORT_DEFAULT_PRIORITY=medium
CSV_IMPORT_DEFAULT_STATUS=new
CSV_IMPORT_DEFAULT_CATEGORY=general

# Working directory (set automatically)
WORKING_DIRECTORY=/path/to/PrismQ_WD
