# Example .env file for Manual Backlog Source

# Database path
MANUAL_BACKLOG_DATABASE_PATH=/path/to/PrismQ_WD/Sources/Internal/ManualBacklog/manual_backlog.db

# Default values for new ideas
MANUAL_BACKLOG_DEFAULT_PRIORITY=medium
MANUAL_BACKLOG_DEFAULT_STATUS=new
MANUAL_BACKLOG_DEFAULT_CATEGORY=general

# Default user (for created_by field)
MANUAL_BACKLOG_DEFAULT_USER=your_username

# Working directory (set automatically)
WORKING_DIRECTORY=/path/to/PrismQ_WD
